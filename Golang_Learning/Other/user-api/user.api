syntax = "v1" //默认版本

info ( //信息
	author: "Diego"
	date:   "20251202"
	desc:   "api示例"
)

type (
	UserInfoReq {
		UserID int64 `json:"user_id"`
	}
	UserInfoResp {
		UserID   int64  `json:"user_id"`
		Nickname string `json:"nickname"`
	}
)

service user-api {
	@doc "获取用户信息"
	@handler UserInfo
	post /user/info (UserInfoReq) returns (UserInfoResp)
}

